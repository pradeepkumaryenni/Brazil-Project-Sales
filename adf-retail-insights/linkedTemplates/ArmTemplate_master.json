{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-retail-insights"
		},
		"LS_ADLSGen2_raw_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'LS_ADLSGen2_raw'"
		},
		"LS_AzureSQL_MetadataDB_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'LS_AzureSQL_MetadataDB'"
		},
		"LS_BlobStorage_landing_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_BlobStorage_landing'"
		},
		"LS_DataBricks_accessToken": {
			"type": "secureString",
			"metadata": "Secure string for 'accessToken' of 'LS_DataBricks'"
		},
		"LS_ADLSGen2_raw_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://stretailinsights.dfs.core.windows.net/"
		},
		"LS_AzureSQL_MetadataDB_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "sql-retail-server-2025.database.windows.net"
		},
		"LS_AzureSQL_MetadataDB_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "sql-retail-metadata"
		},
		"LS_AzureSQL_MetadataDB_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "admin43357028"
		},
		"LS_DataBricks_properties_typeProperties_existingClusterId": {
			"type": "string",
			"defaultValue": "1101-111115-eby6p2k9"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf-retail-insights_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"LS_ADLSGen2_raw_accountKey": {
						"value": "[parameters('LS_ADLSGen2_raw_accountKey')]"
					},
					"LS_AzureSQL_MetadataDB_password": {
						"value": "[parameters('LS_AzureSQL_MetadataDB_password')]"
					},
					"LS_BlobStorage_landing_connectionString": {
						"value": "[parameters('LS_BlobStorage_landing_connectionString')]"
					},
					"LS_DataBricks_accessToken": {
						"value": "[parameters('LS_DataBricks_accessToken')]"
					},
					"LS_ADLSGen2_raw_properties_typeProperties_url": {
						"value": "[parameters('LS_ADLSGen2_raw_properties_typeProperties_url')]"
					},
					"LS_AzureSQL_MetadataDB_properties_typeProperties_server": {
						"value": "[parameters('LS_AzureSQL_MetadataDB_properties_typeProperties_server')]"
					},
					"LS_AzureSQL_MetadataDB_properties_typeProperties_database": {
						"value": "[parameters('LS_AzureSQL_MetadataDB_properties_typeProperties_database')]"
					},
					"LS_AzureSQL_MetadataDB_properties_typeProperties_userName": {
						"value": "[parameters('LS_AzureSQL_MetadataDB_properties_typeProperties_userName')]"
					},
					"LS_DataBricks_properties_typeProperties_existingClusterId": {
						"value": "[parameters('LS_DataBricks_properties_typeProperties_existingClusterId')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}